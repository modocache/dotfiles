[github]
  name = modocache
  # token = API_TOKEN
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  editor = /usr/local/bin/vim
  autocrlf = input
  excludesfile = ~/.cvsignore
[merge]
  tool = vimdiff
[alias]
  cl = clone
  st = status
  unstage = reset HEAD --
  last = log -1 HEAD
  ls = !ls
[commit]
  template = ~/.gitmessage.txt
[help]
  autocorrect = 1
